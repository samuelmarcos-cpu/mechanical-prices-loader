{
  "host": "kbdfans.com",
  "currency": "USD",
  "pseudoUrls": {
    "category": [
      "/collections/[(?!.*products)(?!.*youtube).+]"
    ],
    "product": [
      "/collections/[.+]/products/[(?!.*youtube).+]"
    ]
  },
  "startUrls": [
    "https://kbdfans.com/collections/pcb",
    "https://kbdfans.com/collections/switches",
    "https://kbdfans.com/collections/case",
    "https://kbdfans.com/collections/plate",
    "https://kbdfans.com/collections/keycaps"
  ],
  "selectors": {
    "categories": {
      "selector": "a.btn.btn--tertiary[href]",
      "position": null,
      "attribute": "href",
      "match": {
        "regex": "/collections/(.*)",
        "group": 2
      }
    },
    "sku": {
      "selector": "script[data-productid]",
      "position": 1,
      "attribute": "data-productid",
      "match": null
    },
    "title": {
      "selector": "h1.product-single__title",
      "position": 1,
      "attribute": null,
      "match": null
    },
    "description": {
      "selector": "div.collapsible-content__inner.rte",
      "position": 1,
      "attribute": null,
      "match": null
    },
    "price": {
      "selector": "span[data-product-price]",
      "position": 1,
      "attribute": null,
      "match": {
        "regex": "(?<![\\d,.])\\d{1,3}(,\\d{3})*(\\.(\\d+))?(?![\\d,.]+)",
        "group": 1
      }
    },
    "images": {
      "selector": "div[data-product-photos] img[src]",
      "position": null,
      "attribute": "src",
      "match": null
    }
  },
  "categories": {
    "pcb": [
      "pcb",
      "20",
      "40",
      "60",
      "65",
      "75",
      "80",
      "95",
      "100"
    ],
    "switch": [
      "switches",
      "aliaz-switches",
      "cherry-switches",
      "gateron-swithes",
      "kailh-switches",
      "zealios-switches",
      "ttc"
    ],
    "case": [
      "case",
      "20-layout-case",
      "40-layout-case",
      "60-layout-case",
      "65-layout-case",
      "75-layout-case",
      "80-layout-case",
      "95-layout-case",
      "100-layout-case"
    ],
    "plate": [
      "plate",
      "20-layout-plate",
      "40-layout-plate",
      "60-layout-plate",
      "65-layout-plate",
      "75-layout-plate",
      "80-layout-plate",
      "95-layout-plate",
      "100-layout-plate"
    ],
    "keycap": [
      "keycaps",
      "cherry-profile",
      "sa-profile",
      "dsa-profile",
      "mix-profile",
      "kat-profile",
      "np-profile",
      "da-profile",
      "electrostatic-capacitive-keycaps",
      "oem-profile",
      "osa-profile",
      "enjoypbt-keycaps",
      "maxkey",
      "artisan-keycaps",
      "backlit-keycaps"
    ]
  }
}